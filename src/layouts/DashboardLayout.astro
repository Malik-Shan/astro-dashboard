---
interface Props {
	title: string;
}

const { title } = Astro.props;

import Head from '../components/dashboard/Head.astro';
import Body from '../components/dashboard/Body.astro';

import '../stylesheet/dashboard/base.css'
---
<!doctype html>
<html lang="en">

	<Head title={title}></Head>
	<Body body_height='100vh' topbar_height='60px' sidenav_width='200px'>
		<slot/>
	</Body>

</html>
